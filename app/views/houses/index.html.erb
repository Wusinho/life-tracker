
  <%= turbo_stream_from 'houses' %>

  <%= turbo_frame_tag 'houses' do %>
    <% if @house %>
      <%= render @house %>
      <%= link_to 'Edit', edit_house_path(@house) %>
    <% end %>
  <% end %>


  <% unless @house %>
    <%= turbo_frame_tag 'house_form' do %>
      <%= render 'form', house: @new_house %>
    <% end %>
  <% end %>

